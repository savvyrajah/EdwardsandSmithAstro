{
  "@context": "https://schema.org",
  "@type": "BreadcrumbList",
  "itemListElement": [
    {
      "@type": "ListItem",
      "position": 1,
      "name": "Home",
      "item": "https://www.edwardsandsmith.com.au"
    },
    {
      "@type": "ListItem",
      "position": 2,
      "name": "Recent Purchases",
      "item": "https://www.edwardsandsmith.com.au/recent-purchases"
    },
    {
      "@type": "ListItem",
      "position": 3,
      "name": "Other",
      "item": "https://www.edwardsandsmith.com.au/other/recent-purchases"
    },
    {
      "@type": "ListItem",
      "position": 4,
      "name": "Rasmussen 4815",
      "item": "https://www.edwardsandsmith.com.au/recent-purchases/other/rasmussen-4815"
    }
  ]
}
</script>
